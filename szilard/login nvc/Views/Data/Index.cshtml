@model DataModel[]
@{
    ViewData["Title"] = "Data Page";
}

<table>
    <tr>
        <th>
            Name
        </th>
        <th>
            Value
        </th>
    </tr>

    @for(int i=0;i<Model.Length;i++)
    {
      
        <tr>
            <td>
                @Model[i].Name
            </td>
            <td>
                @Model[i].Value
            </td>
        </tr>

    }
</table>